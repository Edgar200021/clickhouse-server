{
	"name": "clickhouse-server",
	"version": "1.0.0",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"start:local": "tsx watch --env-file=.env.local ./src/server.ts",
		"start:dev": "npm run migration:run && tsx --env-file=.env ./src/server.ts",
		"start:prod": "npm run migration:run && node --env-file=.env dist/src/server.js",
		"build": "rm -rf dist && tsc",
		"test": "docker compose -p clickhouse-server-test --env-file .env.test -f docker-compose.test.yml up --build",
		"test:down": "docker compose -p clickhouse-server-test --env-file .env.test -f docker-compose.test.yml down --rmi all -v",
		"test:local": "npm run vitest",
		"test:coverage": "vitest run --coverage",
		"docker:dev:up": "docker compose -f docker-compose.dev.yml up --build",
		"docker:dev:down": "docker compose -f docker-compose.dev.yml down --rmi all",
		"vitest": "vitest run",
		"kysely:codegen": "kysely-codegen --out-file ./src/types/db/db.ts --camel-case --dialect=postgres --runtime-enums=pascal-case",
		"migration:create": "npx kysely migrate:make",
		"migration:run": "npx kysely migrate:latest",
		"migration:rollback": "npx kysely migrate:rollback --all",
		"migration:list": "npx kysely migrate:list",
		"migration:up": "npx kysely migrate:up",
		"migration:down": "npx kysely migrate:down"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"devDependencies": {
		"@faker-js/faker": "^9.9.0",
		"@tsconfig/node24": "^24.0.1",
		"@types/node": "^24.0.13",
		"@types/nodemailer": "^6.4.17",
		"@types/pg": "^8.15.4",
		"@vitest/coverage-v8": "^3.2.4",
		"kysely-codegen": "^0.18.5",
		"pino-pretty": "^13.0.0",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"vite": "^7.0.4",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@fastify/autoload": "^6.3.1",
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^11.0.1",
		"@fastify/formbody": "^8.0.2",
		"@fastify/helmet": "^13.0.1",
		"@fastify/multipart": "^9.0.3",
		"@fastify/rate-limit": "^10.3.0",
		"@fastify/redis": "^7.0.2",
		"@fastify/sensible": "^6.0.3",
		"@fastify/swagger": "^9.5.1",
		"@fastify/swagger-ui": "^5.2.3",
		"@fastify/type-provider-typebox": "^5.1.0",
		"@sinclair/typebox": "^0.34.37",
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"close-with-grace": "^2.2.0",
		"fastify": "^5.4.0",
		"fastify-cron": "^1.4.0",
		"fastify-plugin": "^5.0.1",
		"kysely": "^0.28.3",
		"kysely-ctl": "^0.14.0",
		"nodemailer": "^7.0.5",
		"pg": "^8.16.3",
		"ts-node": "^10.9.2"
	}
}
